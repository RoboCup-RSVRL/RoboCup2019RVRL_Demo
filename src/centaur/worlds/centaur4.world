<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- A centaur
    <include>
      <uri>model://centaur0</uri>
      <name>robot0</name>
      <pose>0 0 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://centaur1</uri>
      <name>robot1</name>
      <pose>-2 0 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://centaur2</uri>
      <name>robot2</name>
      <pose>0 -2 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://centaur3</uri>
      <name>robot3</name>
      <pose>-2 -2 0 0 0 0</pose>
    </include>
